@model IEnumerable<OdToFood.Data.Models.Restaurant>
@{
    ViewBag.Title = "Home Page";
}
<div class="jumbotron">
        <h1>ASP.NET for Somit</h1>
        <div>
            There are @Model.Count() numbers of restaurants in here .
        </div>
    </div>
    <ul>
        @foreach (var resta in Model)
        {
            <li>@resta.Name</li>
        }
    </ul>